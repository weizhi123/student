<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         id="WebApp_ID" version="3.1">
  <display-name>WebBasic</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>AddServlet</servlet-name>
    <servlet-class>com.jlkj.wz.servlet.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddServlet</servlet-name>
    <url-pattern>/AddServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteServlet</servlet-name>
    <servlet-class>com.jlkj.wz.servlet.DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteServlet</servlet-name>
    <url-pattern>/DeleteServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UpdateServlet</servlet-name>
    <servlet-class>com.jlkj.wz.servlet.UpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateServlet</servlet-name>
    <url-pattern>/UpdateServlet</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>FilterServlet</servlet-name>
    <servlet-class>com.jlkj.wz.servlet.FilterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FilterServlet</servlet-name>
    <url-pattern>/FilterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShowServlet</servlet-name>
    <servlet-class>com.jlkj.wz.servlet.ShowServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowServlet</servlet-name>
    <url-pattern>/ShowServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteBatchStudent</servlet-name>
    <servlet-class>com.jlkj.wz.servlet.DeleteBatchStudent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteBatchStudent</servlet-name>
    <url-pattern>/DeleteBatchStudent</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>StudentTemplateServlet</servlet-name>
    <servlet-class>com.jlkj.wz.servlet.StudentTemplateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentTemplateServlet</servlet-name>
    <url-pattern>/StudentTemplateServlet</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>ImportStudent</servlet-name>
    <servlet-class>com.jlkj.wz.servlet.ImportStudent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImportStudent</servlet-name>
    <url-pattern>/ImportStudent</url-pattern>
  </servlet-mapping>
</web-app>